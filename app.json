{
  "expo": {
    "name": "Gate Entry",
    "slug": "gate-entry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "gateentry",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gateentry.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#9B2C2C",
        "foregroundImage": "./assets/images/logo.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gateentry.app"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FAFAF9",
          "dark": {
            "backgroundColor": "#1C1917"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "09511a6a-5a9e-46b1-9c05-4b25b5d190e5"
      }
    },
    "owner": "tech3440",
    "updates": {
      "url": "https://u.expo.dev/09511a6a-5a9e-46b1-9c05-4b25b5d190e5"
    }
  }
}
